{
    "identifier": "kylefox.github",
    "name": "GitHub",
    "organization": "Kyle Fox",
    "description": "Lorem ipsum, dolor sit amet.",
    "version": "1.0",
    "categories": ["commands"],
    
    "main": "main.js",
    
    "activationEvents": [
        "*"
    ],
    
    "entitlements": {
        "filesystem": "readonly",
        "process": true
    },
    
    "commands": {
        "editor": [
            {
                "title": "View file on GitHub",
                "command": "github.viewFile"
            },
            {
                "title": "View blame on GitHub",
                "command": "github.viewBlame"
            }
        ]
    }
}
